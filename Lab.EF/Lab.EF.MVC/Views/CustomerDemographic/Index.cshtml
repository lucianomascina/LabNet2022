@model List<Lab.EF.MVC.Models.CustomerDemographicView>

@{
    ViewBag.Title = "Customer Demographics";
}

<h2 class="text-center">Customer Demographics</h2>

<hr />

<table class="table table-hover">
    <thead>
        <tr class="active">
            <th>
                Customer Type ID
            </th>
            <th>
                Customer Description
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var customerDemographic in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => customerDemographic.CustomerTypeID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => customerDemographic.CustomerDesc)
                </td>
            </tr>
        }
    </tbody>
</table>
