@model List<Lab.EF.MVC.Models.RegionView>

@{
    ViewBag.Title = "Regions";
}

<h2>Regions</h2>

<hr />

<table class="table table-hover">
    <thead>
        <tr class="info">
            <th>
                Region ID
            </th>
            <th>
                Region Description
            </th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RegionID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RegionDescription)
                </td>

            </tr>
        }
    </tbody>
</table>
