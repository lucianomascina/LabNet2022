@model List<Lab.EF.MVC.Models.TerritoryView>

@{
    ViewBag.Title = "Territories";
}

<h2 class="text-center">Territories</h2>

<hr />

<table class="table table-hover">
    <thead>
        <tr class="active">
            <th>
                Territory ID
            </th>
            <th>
                Territory Description
            </th>
            <th>
                Region ID
            </th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var territory in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => territory.TerritoryID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => territory.TerritoryDescription)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => territory.RegionID)
                </td>
          
            </tr>
        }
    </tbody>
</table>

